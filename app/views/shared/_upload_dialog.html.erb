<div x-dialog x-model="$store.uploadModal.open" style="display: none" class="relative z-10" aria-labelledby="modal-title" role="dialog" aria-modal="true">
  <div x-dialog:panel x-transition class="fixed inset-x-0 bottom-0 px-6 pb-6 overflow-y-auto">
    <div class="pointer-events-auto ml-auto max-w-xl rounded-xl bg-white p-4 shadow-lg ring-1 ring-gray-900/10 z-10 sm:max-w-sm">
      <div class="relative transform overflow-hidden transition-all sm:w-full">
        <div class="absolute top-0 right-0 hidden pt-2 pr-2 sm:block">
          <button @click="$dialog.close()" type="button" class="rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-offset-2">
            <span class="sr-only">Close</span>
            <!-- Heroicon name: outline/x-mark -->
            <svg class="h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
              <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
            </svg>
          </button>
        </div>
        <div class="sm:flex sm:items-start">
          <div class="mt-3 text-center sm:mt-0 sm:ml-4 sm:text-left sm:min-h-52">
            <h3 class="text-lg font-medium leading-6 text-gray-900 mb-2" id="modal-title">Uploads</h3>
            <div class="mt-1">
              <div id="uploads" class="flex flex-col overflow-y-scroll w-64 max-h-80"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>